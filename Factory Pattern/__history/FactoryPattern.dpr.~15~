program FactoryPattern;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,
  PizzaStoreU in 'PizzaStoreU.pas',
  TestU in 'TestU.pas';

begin
  try
    var NYPizzaStore: TPizzaStore := TNYPizzaStore.Create;
    ReadLn;
  except
    on E: Exception do
      Writeln(E.ClassName, ': ', E.Message);
  end;
end.
