program TemplateMethodPattern;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,
  Coffee in 'Coffee.pas',
  Tea in 'Tea.pas',
  CaffeineBeverage in 'CaffeineBeverage.pas';

begin
  try
    var MyTea := TTea.Create;
    MyTea.PrepareRecipe;
    ReadLn;
  except
    on E: Exception do
      Writeln(E.ClassName, ': ', E.Message);
  end;
end.
