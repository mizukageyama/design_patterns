unit ThinCrustDough;

interface

uses
  System.Classes, System.SysUtils, System.Generics.Collections, DoughIntf;

type
  TThinCrustDough = class(TInterfacedObject, IDough)
    function ToString: String;
  end;

implementation

{ TThinCrustDough }

function TThinCrustDough.ToString: String;
begin
  Result :=  'Thin Crust Dough';
end;

end.
