unit DuckCall;

interface

uses
  QuackableIntf;

type
  TDuckCall = class(TInterfacedObject, IQuackable)
  private
    FObservable: IObservable;
  public
    constructor Create;
    procedure Quack;
  end;

implementation

{ TDuckCall }

constructor TDuckCall.Create;
begin
  FObservable := TObservable.Create(Self);
end;

procedure TDuckCall.Quack;
begin
  WriteLn('Kwak');
end;

end.

