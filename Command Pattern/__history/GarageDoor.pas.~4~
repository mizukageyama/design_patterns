unit GarageDoor;

interface

type
  TGarageDoor = class
  public
    procedure Up;
    procedure Down;
    procedure Stop;
    procedure LightOn;
    procedure LightOff;
    constructor Create;
  end;
implementation

{ TGarageDoor }

constructor TGarageDoor.Create;
begin

end;

procedure TGarageDoor.Up;
begin
  WriteLn('Garage Door is Open');
end;

procedure TGarageDoor.Down;
begin
  WriteLn('Garage Door is Close');
end;

procedure TGarageDoor.LightOff;
begin
  WriteLn('Garage Door Light is Off');
end;

procedure TGarageDoor.LightOn;
begin
  WriteLn('Garage Door Light is On');
end;

procedure TGarageDoor.Stop;
begin
  WriteLn('Garage Door is Stopped');
end;

end.

