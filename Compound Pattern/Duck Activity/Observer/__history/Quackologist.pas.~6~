unit Quackologist;

interface

uses
  ObserverIntf;

type
  TQuackologist = class(TInterfacedObject, IObserver)
  public
    procedure Update(Duck: IQuackObservable);
  end;

implementation

{ TQuackologist }

procedure TQuackologist.Update(Duck: IQuackObservable);
begin
  WriteLn('Quackologist: ' + Duck.ToString + ' just quacked.');
end;

end.
