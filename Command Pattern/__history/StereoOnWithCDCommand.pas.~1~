unit StereoOnWithCDCommand;

interface

uses CommandIntf, Stereo;

type
  TStereoOnForCDCommand = class(TInterfacedObject, ICommand)
  private
    FStereo : TStereo;
  public
    constructor Create(AStereo: TStereo);
    procedure Execute;
  end;

implementation

{ TStereoOnForCDCommand }

constructor TStereoOnForCDCommand.Create(AStereo: TStereo);
begin
  FStereo := AStereo;
end;

procedure TStereoOnForCDCommand.Execute;
begin
  FStereo.OnStereo;
  FStereo.SetCD;
  FStereo.SetVolume(11);
end;

end.
