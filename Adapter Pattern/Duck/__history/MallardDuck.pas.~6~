unit MallardDuck;

interface

uses
  DuckIntf, System.SysUtils;

type
  TMallardDuck = class(TInterfacedObject, IDuck)
  public
    procedure Quack;
    procedure Fly;
  end;

implementation

{ TMallardDuck }

procedure TMallardDuck.Fly;
begin
  WriteLn('I''m flying');
end;

procedure TMallardDuck.Quack;
begin
  WriteLn('Quack');
end;

end.
